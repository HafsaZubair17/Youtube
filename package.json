{
  "name": "youtubeui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "node-sass": "^5.0.0",
    "node-sass-chokidar": "^1.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-avatar": "^3.10.0",
    "react-avatar-edit": "^1.0.0",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
